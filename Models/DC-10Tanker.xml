<?xml version="1.0" encoding="UTF-8" ?>

<!-- McDonnell Douglas DC-10 -->
<!-- Main model, DC-10-30 tanker -->

<PropertyList>

	<model>
		<name>Fuselage</name>
		<path>Aircraft/DC-10-30/Models/DC-10-30-fuselage.xml</path>
	</model>
	
	<!-- firetank -->
	<model>
		<path>Aircraft/DC-10-30/Models/firetank.xml</path>
		<offsets>
		  <x-m> 0.00 </x-m>
		  <y-m> 0.00 </y-m>
		  <z-m> 0.10 </z-m>
		</offsets>
	</model>

	<!-- Foam or Water drop -->
	<model>
		<path>Aircraft/DC-10-30/Models/Effects/waterdrop3.xml</path>
		<offsets>
		  <x-m>-3.20 </x-m>
		  <y-m> 0.00 </y-m>
		  <z-m>-3.00 </z-m>
		</offsets>
	</model>
	<model>
		<path>Aircraft/DC-10-30/Models/Effects/waterdrop2.xml</path>
		<offsets>
		  <x-m>-3.20 </x-m>
		  <y-m> 0.00 </y-m>
		  <z-m>-3.00 </z-m>
		</offsets>
	</model>
	<model>
		<path>Aircraft/DC-10-30/Models/Effects/waterdrop1.xml</path>
		<offsets>
		  <x-m>-3.20 </x-m>
		  <y-m> 0.00 </y-m>
		  <z-m>-3.00 </z-m>
		</offsets>
	</model>

	<!-- Retardant drop -->
	<model>
		<path>Aircraft/DC-10-30/Models/Effects/retardantdrop3.xml</path>
		<offsets>
		  <x-m> 2.30 </x-m>
		  <y-m> 0.00 </y-m>
		  <z-m>-3.00 </z-m>
		</offsets>
	</model>
	<model>
		<path>Aircraft/DC-10-30/Models/Effects/retardantdrop2.xml</path>
		<offsets>
		  <x-m>-4.20 </x-m>
		  <y-m> 0.00 </y-m>
		  <z-m>-3.00 </z-m>
		</offsets>
	</model>
	<model>
		<path>Aircraft/DC-10-30/Models/Effects/retardantdrop1.xml</path>
		<offsets>
		  <x-m>-11.00 </x-m>
		  <y-m>  0.00 </y-m>
		  <z-m> -3.00 </z-m>
		</offsets>
	</model>

	<foo include="DC-10Tanker-main.xml" omit-node="y" />

</PropertyList>
