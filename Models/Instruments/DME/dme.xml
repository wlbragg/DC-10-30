<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>dme.ac</path>

	<animation>
		<type>material</type>
		<object-name>Screens</object-name>
		<object-name>DecimalPoints</object-name>
		<object-name>Dist1-.1</object-name>
		<object-name>Dist1-1</object-name>
		<object-name>Dist1-10</object-name>
		<object-name>Dist1-100</object-name>
		<object-name>Dist2-.1</object-name>
		<object-name>Dist2-1</object-name>
		<object-name>Dist2-10</object-name>
		<object-name>Dist2-100</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<name>NAV 1 on/off</name>
		<type>select</type>
		<object-name>Off-1</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/nav[0]</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<name>NAV 2 on/off</name>
		<type>select</type>
		<object-name>Off-2</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/nav[1]</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>Dist1-100</object-name>
		<property>instrumentation/dme[0]/indicated-distance-nm</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Dist1-10</object-name>
		<property>instrumentation/dme[0]/indicated-distance-nm</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Dist1-1</object-name>
		<property>instrumentation/dme[0]/indicated-distance-nm</property>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Dist1-.1</object-name>
		<property>instrumentation/dme[0]/indicated-distance-nm</property>
		<step>0.1</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>Dist2-100</object-name>
		<property>instrumentation/dme[1]/indicated-distance-nm</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Dist2-10</object-name>
		<property>instrumentation/dme[1]/indicated-distance-nm</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Dist2-1</object-name>
		<property>instrumentation/dme[1]/indicated-distance-nm</property>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Dist2-.1</object-name>
		<property>instrumentation/dme[1]/indicated-distance-nm</property>
		<step>0.1</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>

</PropertyList>
